    "currency": ["Währung", "Waehrung"],
    "counterpartName": ["Auftraggeber/Begünstigter"],
    "counterpartIban": ["IBAN"],
    "purpose": ["Verwendungszweck", "Buchungstext"],
    "txType": { "from": "Buchungstext", "lookup": { "Kartenzahlung": "CARD", "Lastschrift": "DD", "Gutschrift": "CRDT" } }
  }
}


